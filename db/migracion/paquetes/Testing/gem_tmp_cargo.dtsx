                <property
                  dataType="System.String"
                  description="Comando SQL para ejecutar."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">
				  SELECT f.id,
       1 AS condicion_cargo,
       NULL AS espacio_curricular_id,
       materia_id,
       materia_nombre,
       turno_id,
       curso_id,
       division_id,
       CASE
           WHEN left(regsaldesc,4)='HORA' THEN oblig
           ELSE 0
       END AS carga_horaria,
       regimen_salarial,
       escid
FROM fila f
JOIN planilla p ON f.planilla_id=p.id
WHERE p.escid IN ('4047','4139','1601')
  AND p.mes=5
  AND p.year=2017
  AND (fila_id IS NULL or fila_id &lt;= 0 or f.id=f.fila_id);</property>
